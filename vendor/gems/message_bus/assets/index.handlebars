<button {{action discover}} {{bindAttr disabled="disabled"}}>Discover Processes</button> 

<table>
  <thead>
    <tr>
      <td>pid</td>
      <td>full_path</td>
      <td>hostname</td>
      <td>uptime</td>
      <td></td>
    </tr>
  </thead>

  <tbody>
  {{#each processes}}
    <tr>
      <td>{{pid}}</td>
      <td>{{full_path}}</td>
      <td>{{hostname}}</td>
      <td>{{uptime}} secs</td>
      <td><button {{action hup this}}>HUP</button></td>
    </tr>
  {{/each}}
  </tbody>
</table>
