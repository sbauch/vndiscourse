<div class='list-controls'>
  <div class="container">
    <ul class="nav nav-pills" id='category-filter'>
      {{#each controller.availableNavItems}}
        {{view Discourse.NavItemView contentBinding="this"}}
      {{/each}}
  <div class='username controls'>
    {{view Discourse.TextField valueBinding="controller.username"  placeholder="Search for a user by name, position, or team..."}}
  </div>
  </ul>

<section class="category-column first">
  {{#each content}}
  <table id='topic-list'>
    <tr>
      <th>
        <a href="/users/{{unbound username_lower}}">{{avatar this imageSize="small"}} {{unbound name}}</a>
      </th>
    </tr>
    <tr>
      <td class='main-link'> <a href="mailto:{{unbound email}}">{{unbound email}}</a></td>
   </tr>
   <tr>
     <td>
              
     </td>
   </tr>  
 </table>
     
     <footer class="clearfix">
       </footer>  
  {{/each}}
</section>


<div class='clearfix'></div>

{{#if content.length}}

{{else}}
  <div>We couldn't find any users!</div>
{{/if}}